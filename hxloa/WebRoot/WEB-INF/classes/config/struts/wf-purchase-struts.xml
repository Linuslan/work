		<action name="queryAuditPage" class="com.linuslan.oa.workflow.flows.purchase.action.PurchaseAction" method="queryAuditPage"></action>
		<action name="queryAuditedPage" class="com.linuslan.oa.workflow.flows.purchase.action.PurchaseAction" method="queryAuditedPage"></action>
		<action name="initSelect" class="com.linuslan.oa.workflow.flows.purchase.action.PurchaseAction" method="initSelect"></action>
		<action name="queryById" class="com.linuslan.oa.workflow.flows.purchase.action.PurchaseAction" method="queryById">
