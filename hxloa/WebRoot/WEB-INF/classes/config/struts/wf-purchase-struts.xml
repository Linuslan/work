		<action name="audit" class="com.linuslan.oa.workflow.flows.purchase.action.PurchaseAction" method="audit"></action>
		<action name="delContentById" class="com.linuslan.oa.workflow.flows.purchase.action.PurchaseAction" method="delContentById"></action>
