<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="supplier" namespace="/workflow/supplier" extends="struts-default">
		<action name="queryPage" class="com.linuslan.oa.workflow.flows.supplier.action.SupplierAction" method="queryPage"></action>
		<action name="queryById" class="com.linuslan.oa.workflow.flows.supplier.action.SupplierAction" method="queryById">
			<result name="add">/com/linuslan/oa/workflow/supplier/Add.jsp</result>
			<result name="edit">/com/linuslan/oa/workflow/supplier/Edit.jsp</result>
		</action>
		<action name="update" class="com.linuslan.oa.workflow.flows.supplier.action.SupplierAction" method="update"></action>
		<action name="add" class="com.linuslan.oa.workflow.flows.supplier.action.SupplierAction" method="add"></action>
		<action name="del" class="com.linuslan.oa.workflow.flows.supplier.action.SupplierAction" method="del"></action>
		<action name="authorize" class="com.linuslan.oa.workflow.flows.supplier.action.SupplierAction" method="authorize"></action>
	</package>
</struts>