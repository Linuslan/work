<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="capital" namespace="/sys/capital" extends="struts-default">
		<action name="queryPage" class="com.linuslan.oa.system.capital.action.CapitalAction" method="queryPage"></action>
		<action name="queryUserPage" class="com.linuslan.oa.system.capital.action.CapitalAction" method="queryUserPage"></action>
		<action name="queryAll" class="com.linuslan.oa.system.capital.action.CapitalAction" method="queryAll"></action>
		<action name="initSelect" class="com.linuslan.oa.system.capital.action.CapitalAction" method="initSelect"></action>
		<action name="initContent" class="com.linuslan.oa.system.capital.action.CapitalAction" method="initContent">
			<result name="add">/com/linuslan/oa/system/capital/Add.jsp</result>
			<result name="edit">/com/linuslan/oa/system/capital/Edit.jsp</result>
			<result name="view">/com/linuslan/oa/system/capital/View.jsp</result>
		</action>
		<action name="update" class="com.linuslan.oa.system.capital.action.CapitalAction" method="update"></action>
		<action name="add" class="com.linuslan.oa.system.capital.action.CapitalAction" method="add"></action>
		<action name="delById" class="com.linuslan.oa.system.capital.action.CapitalAction" method="delById"></action>
