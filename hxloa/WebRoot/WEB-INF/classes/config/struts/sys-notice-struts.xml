<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="notice" namespace="/sys/notice" extends="struts-default">
		<action name="queryPage" class="com.linuslan.oa.system.notice.action.NoticeAction" method="queryPage"></action>
		<action name="queryPageByUserId" class="com.linuslan.oa.system.notice.action.NoticeAction" method="queryPageByUserId"></action>
		<action name="queryAll" class="com.linuslan.oa.system.notice.action.NoticeAction" method="queryAll"></action>
		<action name="queryById" class="com.linuslan.oa.system.notice.action.NoticeAction" method="queryById">
			<result name="add">/com/linuslan/oa/system/notice/Add.jsp</result>
			<result name="edit">/com/linuslan/oa/system/notice/Edit.jsp</result>
			<result name="view">/com/linuslan/oa/system/notice/View.jsp</result>
		</action>
		<action name="update" class="com.linuslan.oa.system.notice.action.NoticeAction" method="update"></action>
		<action name="add" class="com.linuslan.oa.system.notice.action.NoticeAction" method="add"></action>
		<action name="del" class="com.linuslan.oa.system.notice.action.NoticeAction" method="del"></action>
		<action name="send" class="com.linuslan.oa.system.notice.action.NoticeAction" method="send"></action>
	</package>
</struts>