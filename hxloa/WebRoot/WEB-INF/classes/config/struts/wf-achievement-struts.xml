		<action name="queryScorePage" class="com.linuslan.oa.workflow.flows.achievement.action.AchievementAction" method="queryScorePage"></action>
		<action name="queryReportPage" class="com.linuslan.oa.workflow.flows.achievement.action.AchievementAction" method="queryReportPage"></action>
		<action name="initSelect" class="com.linuslan.oa.workflow.flows.achievement.action.AchievementAction" method="initSelect"></action>
		<action name="queryById" class="com.linuslan.oa.workflow.flows.achievement.action.AchievementAction" method="queryById">
			<result name="add">/com/linuslan/oa/workflow/achievement/Add.jsp</result>
			<result name="edit">/com/linuslan/oa/workflow/achievement/Edit.jsp</result>
			<result name="view">/com/linuslan/oa/workflow/achievement/View.jsp</result>
			<result name="audit">/com/linuslan/oa/workflow/achievement/Audit.jsp</result>
			<result name="selfScore">/com/linuslan/oa/workflow/achievement/SelfScore.jsp</result>
			<result name="leaderScore">/com/linuslan/oa/workflow/achievement/LeaderScore.jsp</result>
			<result name="finish">/com/linuslan/oa/workflow/achievement/Finish.jsp</result>
