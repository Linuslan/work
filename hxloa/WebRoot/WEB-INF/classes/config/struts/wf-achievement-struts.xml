		<action name="update" class="com.linuslan.oa.workflow.flows.achievement.action.AchievementAction" method="update"></action>
		<action name="del" class="com.linuslan.oa.workflow.flows.achievement.action.AchievementAction" method="del"></action>
		<action name="commit" class="com.linuslan.oa.workflow.flows.achievement.action.AchievementAction" method="commit"></action>
		<action name="audit" class="com.linuslan.oa.workflow.flows.achievement.action.AchievementAction" method="audit"></action>
		<action name="auditBatch" class="com.linuslan.oa.workflow.flows.achievement.action.AchievementAction" method="auditBatch"></action>
		<action name="delContentById" class="com.linuslan.oa.workflow.flows.achievement.action.AchievementAction" method="delContentById"></action>
		<action name="exportYearAchievement" class="com.linuslan.oa.workflow.flows.achievement.action.AchievementAction" method="exportYearAchievement"></action>
		<action name="copy" class="com.linuslan.oa.workflow.flows.achievement.action.AchievementAction" method="copy"></action>
