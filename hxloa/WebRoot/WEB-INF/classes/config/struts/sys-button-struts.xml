<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="button" namespace="/sys/button" extends="struts-default">
		<action name="queryPageByMenuId" class="com.linuslan.oa.system.button.action.ButtonAction" method="queryPageByMenuId"></action>
		<action name="queryById" class="com.linuslan.oa.system.button.action.ButtonAction" method="queryById">
			<result name="add">/com/linuslan/oa/system/button/Add.jsp</result>
			<result name="edit">/com/linuslan/oa/system/button/Edit.jsp</result>
		</action>
		<action name="update" class="com.linuslan.oa.system.button.action.ButtonAction" method="update"></action>
		<action name="add" class="com.linuslan.oa.system.button.action.ButtonAction" method="add"></action>
		<action name="del" class="com.linuslan.oa.system.button.action.ButtonAction" method="del"></action>
	</package>
</struts>