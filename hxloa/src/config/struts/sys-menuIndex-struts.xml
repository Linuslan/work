<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="menuIndex" namespace="/sys/menuIndex" extends="struts-default">
		<action name="queryPageByMenuId" class="com.linuslan.oa.system.menu.action.MenuIndexAction" method="queryPageByMenuId"></action>
		<action name="queryById" class="com.linuslan.oa.system.menu.action.MenuIndexAction" method="queryById">
			<result name="add">/com/linuslan/oa/system/menuIndex/Add.jsp</result>
			<result name="edit">/com/linuslan/oa/system/menuIndex/Edit.jsp</result>
		</action>
		<action name="update" class="com.linuslan.oa.system.menu.action.MenuIndexAction" method="update"></action>
		<action name="add" class="com.linuslan.oa.system.menu.action.MenuIndexAction" method="add"></action>
		<action name="del" class="com.linuslan.oa.system.menu.action.MenuIndexAction" method="del"></action>
		<action name="queryIndex" class="com.linuslan.oa.system.menu.action.MenuIndexAction" method="queryIndex"></action>
	</package>
</struts>