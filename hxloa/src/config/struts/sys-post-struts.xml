<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="post" namespace="/sys/post" extends="struts-default">
		<action name="queryPageByDepartmentId" class="com.linuslan.oa.system.post.action.PostAction" method="queryPageByDepartmentId"></action>
		<action name="queryById" class="com.linuslan.oa.system.post.action.PostAction" method="queryById">
			<result name="add">/com/linuslan/oa/system/post/Add.jsp</result>
			<result name="edit">/com/linuslan/oa/system/post/Edit.jsp</result>
		</action>
		<action name="update" class="com.linuslan.oa.system.post.action.PostAction" method="update"></action>
		<action name="add" class="com.linuslan.oa.system.post.action.PostAction" method="add"></action>
		<action name="del" class="com.linuslan.oa.system.post.action.PostAction" method="del"></action>
		<action name="queryByDepartmentId" class="com.linuslan.oa.system.post.action.PostAction" method="queryByDepartmentId"></action>
	</package>
</struts>