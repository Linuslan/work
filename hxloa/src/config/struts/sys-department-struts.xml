<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="department" namespace="/sys/department" extends="struts-default">
		<action name="queryTree" class="com.linuslan.oa.system.department.action.DepartmentAction" method="queryTree"></action>
		<action name="queryById" class="com.linuslan.oa.system.department.action.DepartmentAction" method="queryById">
			<result name="add">/com/linuslan/oa/system/department/Add.jsp</result>
			<result name="edit">/com/linuslan/oa/system/department/Edit.jsp</result>
			<result name="addClass">/com/linuslan/oa/system/department/AddClass.jsp</result>
		</action>
		<action name="add" class="com.linuslan.oa.system.department.action.DepartmentAction" method="add"></action>
		<action name="update" class="com.linuslan.oa.system.department.action.DepartmentAction" method="update"></action>
		<action name="del" class="com.linuslan.oa.system.department.action.DepartmentAction" method="del"></action>
		<action name="addClass" class="com.linuslan.oa.system.department.action.DepartmentAction" method="addClass"></action>
		<action name="delClass" class="com.linuslan.oa.system.department.action.DepartmentAction" method="delClass"></action>
	</package>
</struts>