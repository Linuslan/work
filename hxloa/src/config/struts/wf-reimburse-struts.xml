			<result name="add">/com/linuslan/oa/workflow/reimburse/Add.jsp</result>
			<result name="edit">/com/linuslan/oa/workflow/reimburse/Edit.jsp</result>
			<result name="view">/com/linuslan/oa/workflow/reimburse/View.jsp</result>
			<result name="audit">/com/linuslan/oa/workflow/reimburse/Audit.jsp</result>
			<result name="print">/com/linuslan/oa/workflow/reimburse/Print.jsp</result>
		</action>
		<action name="queryPrintList" class="com.linuslan.oa.workflow.flows.reimburse.action.ReimburseAction" method="queryPrintList">
			<result name="success">/com/linuslan/oa/workflow/reimburse/PrintList.jsp</result>
		</action>
		<action name="queryContentsByReimburseId" class="com.linuslan.oa.workflow.flows.reimburse.action.ReimburseAction" method="queryContentsByReimburseId"></action>
		<action name="queryReimburseClassesByDepartmentId" class="com.linuslan.oa.workflow.flows.reimburse.action.ReimburseAction" method="queryReimburseClassesByDepartmentId"></action>
		<action name="add" class="com.linuslan.oa.workflow.flows.reimburse.action.ReimburseAction" method="add"></action>
		<action name="update" class="com.linuslan.oa.workflow.flows.reimburse.action.ReimburseAction" method="update"></action>
		<action name="del" class="com.linuslan.oa.workflow.flows.reimburse.action.ReimburseAction" method="del"></action>
		<action name="commit" class="com.linuslan.oa.workflow.flows.reimburse.action.ReimburseAction" method="commit"></action>
		<action name="audit" class="com.linuslan.oa.workflow.flows.reimburse.action.ReimburseAction" method="audit"></action>
		<action name="delContentById" class="com.linuslan.oa.workflow.flows.reimburse.action.ReimburseAction" method="delContentById"></action>
