<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="reimburse" namespace="/workflow/reimburse" extends="struts-default">
		<action name="queryPage" class="com.linuslan.oa.workflow.flows.reimburse.action.ReimburseAction" method="queryPage"></action>
		<action name="queryAuditPage" class="com.linuslan.oa.workflow.flows.reimburse.action.ReimburseAction" method="queryAuditPage"></action>
		<action name="queryAuditedPage" class="com.linuslan.oa.workflow.flows.reimburse.action.ReimburseAction" method="queryAuditedPage"></action>
		<action name="queryReportPage" class="com.linuslan.oa.workflow.flows.reimburse.action.ReimburseAction" method="queryReportPage"></action>
		<action name="sumReportPage" class="com.linuslan.oa.workflow.flows.reimburse.action.ReimburseAction" method="sumReportPage"></action>
		<action name="initSelect" class="com.linuslan.oa.workflow.flows.reimburse.action.ReimburseAction" method="initSelect"></action>
