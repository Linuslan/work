<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="company" namespace="/sys/company" extends="struts-default">
		<action name="queryPage" class="com.linuslan.oa.system.company.action.CompanyAction" method="queryPage"></action>
		<action name="queryAll" class="com.linuslan.oa.system.company.action.CompanyAction" method="queryAll"></action>
		<action name="queryById" class="com.linuslan.oa.system.company.action.CompanyAction" method="queryById">
			<result name="add">/com/linuslan/oa/system/company/Add.jsp</result>
			<result name="edit">/com/linuslan/oa/system/company/Edit.jsp</result>
		</action>
		<action name="update" class="com.linuslan.oa.system.company.action.CompanyAction" method="update"></action>
		<action name="add" class="com.linuslan.oa.system.company.action.CompanyAction" method="add"></action>
		<action name="del" class="com.linuslan.oa.system.company.action.CompanyAction" method="del"></action>
	</package>
</struts>